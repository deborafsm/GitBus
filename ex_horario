#0 11
#12 17
#18 23

horario = input("digite um horario")
format_horario= int(horario)
bom_dia = 0 <= format_horario < 11
boa_tarde = 12 <= format_horario < 17
boa_noite = 18 <= format_horario < 23


try:
	if bom_dia:
		print("bom dia!")
	elif boa_tarde:
		print("boa tarde")
	elif boa_noite:
		print("boa noite")
	else:
		print("digite um horario valido")
except:
	print("tente novamente ")